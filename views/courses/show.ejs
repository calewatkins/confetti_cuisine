<h1>Course Data for <%= course.name %></h1>
 <table>
  <tr>
    <th>Tile</th>
    <td><%= course.title %></td>
  </tr>
  <tr>
    <th>Description</th>
    <td><%= course.description %></td>
  </tr>
  <tr>
    <th>Max Students</th>
    <td><%= course.maxStudents%></td>
  </tr>
  <tr>
    <th>Cost</th>
    <td><%= course.cost%></td>
  </tr>
 </table>

 <div>
  <a href="/courses">View all courses</a>
</div>